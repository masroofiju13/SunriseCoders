{
  "version": 2,
  "buildCommand": "vite build --config vite.config.vercel.ts --mode production",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3.0.7"
    }
  }
}